{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "api/**",
        "run": {
          "serviceId": "heroes-api-service",
          "region": "europe-west1"
        }
      }      
    ]
  }
}
